
@{
    ViewBag.Title = "GetViewExample3";
}

<h2>GetViewExample3</h2>

<table class="table table-bordered table-condensed table-striped">
    <tr>
        <th>EmpId</th>
        <th>EmpName</th>
        <th>EmpSalary</th>
    </tr>

  

        @foreach (var item in ViewBag.listEmp)//3
        {
            <tr>
                <td> @item.EmpIds</td>
                <td> @item.EmpName</td>
                <td> @item.EmpSalary</td>
            </tr>
        }
           
</table>