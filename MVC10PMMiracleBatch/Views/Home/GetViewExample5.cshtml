@model List<MVC10PMMiracleBatch.Models.EmployeeModel>


<table class="table table-bordered table-striped">
    <tr>
        <th>id</th>
        <th>Name</th>
        <th>Salary</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.EmpId</td>
            <td>@item.EmpName</td>
            <td>@item.EmpSalary</td>
        </tr>
    }
</table>